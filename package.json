{
  "name": "awesome-lexical-react-editor",
  "version": "0.34.0",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "lexical",
    "react",
    "react component",
    "editor",
    "rich text"
  ],
  "author": "z304307393@163.com",
  "license": "MIT",
  "homepage": "https://github.com/zzzzzzzcccccc/package-lexical-editor",
  "repository": {
    "type": "git",
    "url": "https://github.com/zzzzzzzcccccc/package-lexical-editor.git"
  },
  "exports": {
    "import": "./dist/index.es.js",
    "require": "./dist/index.cjs.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:editor": "vite build --config vite.config.editor.ts",
    "publish:npm": "npm run build:editor && npm publish --access public"
  },
  "dependencies": {
    "@lexical/html": "^0.34.0",
    "@lexical/link": "^0.34.0",
    "@lexical/list": "^0.34.0",
